# =============================================================================
# Root .env file for Docker Compose
# =============================================================================
# This file provides API keys to docker-compose.yml
# Copy to .env and fill in your keys
# =============================================================================

# OpenAI API Key (Required for AI-powered brand analysis)
# Get at: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Google API Key (Recommended for SEO analysis)
# Get at: https://console.cloud.google.com/apis/credentials
GOOGLE_API_KEY=

# Google Custom Search Engine ID (Recommended for SERP analysis)
# Create at: https://programmablesearchengine.google.com/
GOOGLE_SEARCH_ENGINE_ID=

# Twitter Bearer Token (Optional - for social media analysis)
# Get at: https://developer.twitter.com/en/portal/dashboard
TWITTER_BEARER_TOKEN=

# Apify Token (Optional - for advanced social scraping)
# Get at: https://console.apify.com/account/integrations
APIFY_API_TOKEN=
